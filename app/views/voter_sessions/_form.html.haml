= form_for @voter_session do |f|
  - if @voter_session.errors.any?
    #error_explanation
      %h2= "#{pluralize(@voter_session.errors.count, "error")} prohibited this voter_session from being saved:"
      %ul
        - @voter_session.errors.full_messages.each do |message|
          %li= message

  .actions
    = f.submit 'Save'
