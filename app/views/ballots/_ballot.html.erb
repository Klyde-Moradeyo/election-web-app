<div id="<%= dom_id ballot %>">
  <table class="table">
  <thead>
    <tr>
      <th scope="col">User ID</th>
      <th scope="col">Ballot ID</th>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
      <th scope="col">start_date</th>
      <th scope="col">end_date</th>
      <th scope="col">show_voters_results</th>
      <th scope="col">access_token</th>
      <th scope="col">voting_type</th>
      <th scope="col">expected_voters</th>
      <th scope="col">Host</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= ballot.user_id %></td>
      <td><%= ballot.id %></td>
      <td><%= ballot.title %></td>
      <td><%= ballot.description %></td>
      <td><%= ballot.start_date %></td>
      <td><%= ballot.end_date %></td>
      <td><%= ballot.show_voter_results %></td>
      <td><%= ballot.access_token %></td>
      <td><%= ballot.voting_type %></td>
      <td><%= ballot.expected_voters %></td>
      <td><%= ballot.host.pluck(:email, :name) %></td>
    </tr>
  </tbody>
</table>

</div>
